{
  "name": "turborepo-starter",
  "description": "",
  "version": "0.0.1",
  "author": "sabishii1",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
